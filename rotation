#include<iostream>
using namespace std;
void fnrotate(int&,int&);
void fnrotate(int&,int&,int&);
void fnrotate(int&,int&,int&,int&);
void fnrotate(double&,double&,double&);
int main(void)
{
	int a=1,b=2,c=3,d=4;
	double x=1.1,y=1.2,z=1.3;
	
	
	cout<<"\nrotate2";
	cout<<"a="<<a<<"\n";
	cout<<"b="<<b<<"\n";
	fnrotate(a,b);
	cout<<"a="<<a<<"\n";
	cout<<"b="<<b<<"\n";
	
	
	
	a=1,b=2,c=3,d=4;
	cout<<"\nrotate 3\n";
	cout<<"a="<<a<<"\n";
	cout<<"b="<<b<<"\n";
	cout<<"c="<<c<<"\n";
	a=1,b=2,c=3,d=4;
	
	
	
	cout<<"\nrotate 4\n";
	cout<<"a="<<a<<"\n";
	cout<<"b="<<b<<"\n";
	cout<<"c="<<c<<"\n";
	cout<<"d="<<d<<"\n";
	
	
	fnrotate(a,b,c,d);
	cout<<"a="<<a<<"\n";
	cout<<"b="<<b<<"\n";
	cout<<"c="<<c<<"\n";
	cout<<"d="<<d<<"\n";
	
	
	cout<<"\n rotate double\n";
	cout<<"x="<<x<<"\n";
	cout<<"y="<<y<<"\n";
	cout<<"z="<<z<<"\n";
	
	fnrotate(x,y,z);
	cout<<"x="<<x<<"\n";
	cout<<"y="<<y<<"\n";
	cout<<"z="<<z<<"\n";
	
	return 0;
	
}

void fnrotate(int &p,int &q)
    {
    	int t;
    	t=p;p=q;q=t;
    }
    
void fnrotate(int &p,int &q,int &r)
    {
    	int t;
    	t=p;p=q;q=r;r=t;
    }
void fnrotate(int &p,int &q,int &r,int &s)
    {
    	int t;
    	t=p;p=q;q=r;r=s;s=t;
    	
    }
    
void fnrotate(double &p,double &q,double &r)
    {
    	double t;
    	t=p;p=q;q=r;r=t;
    }

